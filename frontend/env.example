# Frontend Environment Configuration
# Copy this file to .env and update the values for your environment

# API Configuration
# For development: Use your local IP address (e.g., http://192.168.254.140:3000/api)
# For production: Use your Vercel deployment URL (e.g., https://your-app.vercel.app/api)
EXPO_PUBLIC_API_URL=http://192.168.254.140:3000/api

# Optional: Enable debug logging
EXPO_PUBLIC_DEBUG_MODE=true

# Optional: App configuration
EXPO_PUBLIC_APP_NAME=PetHub
EXPO_PUBLIC_APP_VERSION=1.0.0

# Google Maps API Key (Required for maps functionality)
# Get your API key from: https://console.cloud.google.com/apis/credentials
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
